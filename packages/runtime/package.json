{
  "name": "@deco/workers-runtime",
  "version": "1.0.0",
  "type": "module",
  "files": [
    "dist",
    "package.json",
    "config-schema.json"
  ],
  "scripts": {
    "build": "bun run esbuild.config.ts",
    "dev": "bun run esbuild.config.ts --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250617.0",
    "zod": "^3.25.67",
    "@mastra/core": "0.12.1",
    "@modelcontextprotocol/sdk": "1.17.1",
    "@deco/mcp": "npm:@jsr/deco__mcp@0.5.5",
    "@mastra/cloudflare-d1": "0.12.4",
    "jose": "^6.0.11",
    "zod-to-json-schema": "^3.24.4",
    "@dmitryrechkin/json-schema-to-zod": "1.0.1",
    "drizzle-orm": "^0.44.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./d1": {
      "types": "./dist/d1.d.ts",
      "import": "./dist/d1.js",
      "default": "./dist/d1.js"
    },
    "./proxy": {
      "types": "./dist/proxy.d.ts",
      "import": "./dist/proxy.js",
      "default": "./dist/proxy.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "default": "./dist/client.js"
    },
    "./mastra": {
      "types": "./dist/mastra.d.ts",
      "import": "./dist/mastra.js",
      "default": "./dist/mastra.js"
    },
    "./drizzle": {
      "types": "./dist/drizzle.d.ts",
      "import": "./dist/drizzle.js",
      "default": "./dist/drizzle.js"
    }
  },
  "devDependencies": {
    "esbuild": "^0.25.9",
    "ts-json-schema-generator": "^2.4.0",
    "wrangler": "^4.13.2"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
